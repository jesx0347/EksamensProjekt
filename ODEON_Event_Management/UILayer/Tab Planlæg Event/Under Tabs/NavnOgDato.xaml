<Page x:Class="UILayer.Tab_Planlæg_Event.Under_Tabs.NavnOgDato"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UILayer.Tab_Planlæg_Event.Under_Tabs"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NavnOgDato">

    <Grid>
        <Label x:Name="Label_Navn" Content="Event Navn" HorizontalAlignment="Left" Margin="334,105,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBox_EventNavn" HorizontalAlignment="Left" Height="23" Margin="409,109,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Text="Indtast Navn..." FontStyle="Italic" TextChanged="TextBox_EventNavn_TextChanged"/>
        <Label x:Name="Label_Vælg_Dato" Content="Vælg afviklingsdato" HorizontalAlignment="Left" Margin="291,175,0,0" VerticalAlignment="Top"/>
        <DatePicker x:Name="D" HorizontalAlignment="Left" Margin="409,177,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="Button_Tilføj_Flere_Datoer" Content="Tilføj flere datoer" HorizontalAlignment="Left" Margin="600,178,0,0" VerticalAlignment="Top" Width="100" Click="Button_Tilføj_Flere_Datoer_Click"/>

    </Grid>
</Page>
