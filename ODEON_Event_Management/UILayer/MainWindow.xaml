<Window x:Class="UILayer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UILayer"
        mc:Ignorable="d"
        Title="ODEON Event Manager" Height="450" Width="800">
    <Grid>
        <StackPanel HorizontalAlignment="Left" Height="399" VerticalAlignment="Top" Width="100" Margin="0,10,0,0" Background="Gray" >
            <Button x:Name="Tab_Button_Vis_Events" Content="Vis Events" Height="33" VerticalAlignment="Top" Width="100" Click="ButtonVisEvents_Click" />
            <Button x:Name="Tab_Button_Planlæg_Event" Content="Planlæg Event" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Height="34" Click="ButtonPlanlægEvent_Click"/>
            <Button x:Name="Tab_Button_Vis_Enkelt_Event" Content="Vis Enkelt Event" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Height="33" Click="ButtonVisEvents_Click"/>
        </StackPanel>

    </Grid>
</Window>
